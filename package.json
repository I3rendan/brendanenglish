{
  "name": "brendanenglish",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "element-class": "^0.2.2",
    "parallax-js": "^3.1.0",
    "react": "^15.6.1",
    "react-body-classname": "^1.2.0",
    "react-dom": "^15.6.1",
    "react-fullpage": "I3rendan/react-fullpage",
    "react-g-analytics": "^0.4.2",
    "react-global-configuration": "^1.2.0",
    "react-google-charts": "^1.5.5",
    "react-image-lightbox": "^4.1.0",
    "react-preload": "^0.5.1",
    "react-router-dom": "^4.1.1",
    "timeago-react": "^1.2.2"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
